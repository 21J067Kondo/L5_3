<%= form_tag '/titter/regist', method: :post, data: { turbo: false } do %>
    <%= label_tag :uid, 'ユーザーID' %>
    <%= text_field_tag :uid %><br>
    <%= label_tag :pass, 'パスワード' %>
    <%= password_field_tag :pass %><br>
    <%= label_tag :password_confirmation, 'パスワード(確認)' %>
    <%= password_field_tag :password_confirmation %><br>
    <%= submit_tag '登録' %>
<% end %>
<%= flash[:notice] %>